!_TAG_FILE_FORMAT	2	/extended format; --format=1 will not append ;" to lines/
!_TAG_FILE_SORTED	1	/0=unsorted, 1=sorted, 2=foldcase/
!_TAG_PROGRAM_AUTHOR	Darren Hiebert	/dhiebert@users.sourceforge.net/
!_TAG_PROGRAM_NAME	Exuberant Ctags	//
!_TAG_PROGRAM_URL	http://ctags.sourceforge.net	/official site/
!_TAG_PROGRAM_VERSION	5.8	//
CFLAGS	Makefile	/^CFLAGS=-Wall -O$/;"	m
CPP11	Makefile	/^CPP11=-std=c++11$/;"	m
CURSES	Makefile	/^CURSES=-lncurses$/;"	m
OBJS	Makefile	/^OBJS=$/;"	m
Tile	a.cpp	/^	Tile (int id, bool isMountain, bool isForest) :$/;"	f	class:Tile
Tile	a.cpp	/^class Tile {$/;"	c	file:
id	a.cpp	/^	int id;$/;"	m	class:Tile	file:
isForest	a.cpp	/^	bool isForest;$/;"	m	class:Tile	file:
isMountain	a.cpp	/^	bool isMountain;$/;"	m	class:Tile	file:
main	a.cpp	/^int main (void) $/;"	f
printSquare	a.cpp	/^	static void printSquare (std::vector<Tile> tiles, int width) \/\/ FIXME$/;"	f	class:Tile
